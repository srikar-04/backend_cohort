generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id          Int      @id @default(autoincrement())
  email       String   @unique
  name        String
  created_at  DateTime @default(now())
}

model Chat {
  id          Int      @id @default(autoincrement())
  name        String
  created_at  DateTime @default(now())  
}

model Document {
  id          Int     @id @default(autoincrement())
  name        String
  created_at  DateTime @default(now())
}